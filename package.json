{
  "name": "kiti-front",
  "version": "0.0.1",
  "description": "Sport results and statistics service",
  "productName": "Kiti",
  "author": "Jukka Karvonen <jukka.karvonen@iki.fi>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:e2e": "start-server-and-test dev http://localhost:9000/ cy:run",
    "cy:run": "cypress run --headless --browser chrome",
    "cy:open": "cypress open",
    "dev": "quasar dev",
    "build": "quasar build",
    "postinstall": "quasar prepare"
  },
  "dependencies": {
    "@quasar/extras": "^1.17.0",
    "axios": "^1.12.2",
    "pinia": "^3.0.3",
    "vue": "^3.4.18",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@intlify/unplugin-vue-i18n": "^11.0.1",
    "@quasar/app-vite": "^2.4.0",
    "@types/node": "^24.7.2",
    "@types/vue-router": "^2.0.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "cypress": "^15.4.0",
    "eslint": "^9.37.0",
    "eslint-plugin-vue": "^10.5.0",
    "globals": "^16.4.0",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.2",
    "quasar": "^2.18.5",
    "start-server-and-test": "^2.1.2",
    "typescript": "^5.9.3",
    "vite-plugin-checker": "^0.11.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.1"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
